{
   "name": "cego-api",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/express": "^4.17.1",
      "@types/jsonwebtoken": "^8.3.3",
      "@types/node": "^10.14.17",
      "ts-node": "^8.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "bcrypt": "^3.0.6",
      "body-parser": "^1.18.1",
      "express": "^4.15.4",
      "jsonwebtoken": "^8.5.1",
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.18"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "build": "tsc --sourceMap false",
      "migrateup": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "migratedown": "ts-node ./node_modules/typeorm/cli.js migration:revert",
      "generate": "ts-node ./node_modules/typeorm/cli.js migration:create -n InitialUserData"
   }
}

{
   "name": "cego-api",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "ts-node": "3.3.0",
      "@types/node": "^8.0.29",
      "@types/express": "^4.17.1",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "typeorm": "0.2.18",
      "reflect-metadata": "^0.1.10",
      "mysql": "^2.14.1",
      "express": "^4.15.4",
      "body-parser": "^1.18.1"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "migrateup": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "migratedown": "ts-node ./node_modules/typeorm/cli.js migration:revert",
      "generate": "ts-node ./node_modules/typeorm/cli.js migration:create -n CustomForeignKeys"
   }
}
